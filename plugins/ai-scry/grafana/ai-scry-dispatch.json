{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "-- Grafana --" },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": true,
  "panels": [
    {
      "title": "Session Stats",
      "type": "stat",
      "gridPos": { "h": 4, "w": 24, "x": 0, "y": 0 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "refId": "Agents",
          "expr": "sum(count_over_time({service_name=\"ai-scry\"} | json | event_type=\"agent_complete\" [$__range]))",
          "legendFormat": "Total Agents"
        },
        {
          "refId": "Tools",
          "expr": "sum(count_over_time({service_name=\"ai-scry\"} | json | event_type=\"tool_start\" [$__range]))",
          "legendFormat": "Total Tools"
        },
        {
          "refId": "Errors",
          "expr": "sum(count_over_time({service_name=\"ai-scry\"} | json | event_type=\"tool_error\" [$__range]))",
          "legendFormat": "Errors"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 50 }
            ]
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Errors" },
            "properties": [
              { "id": "thresholds", "value": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "red", "value": 1 }] } }
            ]
          }
        ]
      },
      "options": {
        "reduceOptions": { "calcs": ["sum"], "fields": "", "values": false },
        "orientation": "horizontal",
        "textMode": "auto",
        "colorMode": "background"
      }
    },
    {
      "title": "Agent Dispatch Table",
      "type": "table",
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 4 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "refId": "A",
          "expr": "{service_name=\"ai-scry\"} | json | event_type=\"agent_complete\"",
          "legendFormat": ""
        }
      ],
      "transformations": [
        {
          "id": "extractFields",
          "options": {
            "source": "Line",
            "format": "json"
          }
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Line": true, "id": true, "labels": true, "tsNs": true, "Time": false },
            "indexByName": {},
            "renameByName": {
              "agent_id": "Agent ID",
              "parent_agent_id": "Parent",
              "agent_label": "Type",
              "duration_ms": "Duration (ms)",
              "tokens_in": "Tokens In",
              "tokens_out": "Tokens Out",
              "status": "Status",
              "model": "Model",
              "task_description": "Task"
            }
          }
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Task" },
            "properties": [
              { "id": "custom.width", "value": 300 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Duration (ms)" },
            "properties": [
              { "id": "unit", "value": "ms" }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": [{ "displayName": "Time", "desc": true }]
      }
    },
    {
      "title": "Agent Duration",
      "type": "bargauge",
      "gridPos": { "h": 12, "w": 12, "x": 0, "y": 14 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (agent_label) (sum_over_time({service_name=\"ai-scry\"} | json | event_type=\"agent_complete\" | unwrap duration_ms [$__range]))",
          "legendFormat": "{{agent_label}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "ms",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 30000 },
              { "color": "red", "value": 120000 }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["sum"], "fields": "", "values": false },
        "orientation": "horizontal",
        "displayMode": "gradient",
        "showUnfilled": true
      }
    },
    {
      "title": "Spawn Rate",
      "type": "timeseries",
      "gridPos": { "h": 12, "w": 12, "x": 12, "y": 14 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(count_over_time({service_name=\"ai-scry\"} | json | event_type=\"tool_start\" | tool_name=\"Task\" [$__interval]))",
          "legendFormat": "Agent Spawns"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 60,
            "gradientMode": "none",
            "lineWidth": 1,
            "barAlignment": 0,
            "showPoints": "never",
            "stacking": { "mode": "none" }
          },
          "unit": "short"
        }
      },
      "options": {
        "legend": { "displayMode": "hidden" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "title": "Tool Timeline",
      "type": "logs",
      "gridPos": { "h": 10, "w": 16, "x": 0, "y": 26 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "refId": "A",
          "expr": "{service_name=\"ai-scry\"} | json | event_type=~\"tool_start|tool_end\"",
          "legendFormat": ""
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": false,
        "showCommonLabels": false,
        "wrapLogMessage": true,
        "prettifyLogMessage": true,
        "enableLogDetails": true,
        "sortOrder": "Descending",
        "dedupStrategy": "none"
      }
    },
    {
      "title": "Tool Distribution",
      "type": "piechart",
      "gridPos": { "h": 10, "w": 8, "x": 16, "y": 26 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (tool_name) (count_over_time({service_name=\"ai-scry\"} | json | event_type=\"tool_start\" [$__range]))",
          "legendFormat": "{{tool_name}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" }
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["sum"], "fields": "", "values": false },
        "pieType": "donut",
        "tooltip": { "mode": "single" },
        "legend": { "displayMode": "table", "placement": "right", "values": ["value", "percent"] }
      }
    },
    {
      "title": "Token Budget by Agent",
      "type": "bargauge",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 36 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (agent_label) (sum_over_time({service_name=\"ai-scry\"} | json | event_type=\"agent_complete\" | unwrap tokens_in [$__range]))",
          "legendFormat": "{{agent_label}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 50000 },
              { "color": "red", "value": 200000 }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["sum"], "fields": "", "values": false },
        "orientation": "horizontal",
        "displayMode": "gradient",
        "showUnfilled": true
      }
    },
    {
      "title": "Error Stream",
      "type": "logs",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 36 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "refId": "A",
          "expr": "{service_name=\"ai-scry\"} | json | event_type=\"tool_error\"",
          "legendFormat": ""
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": false,
        "showCommonLabels": false,
        "wrapLogMessage": true,
        "prettifyLogMessage": true,
        "enableLogDetails": true,
        "sortOrder": "Descending",
        "dedupStrategy": "none"
      }
    }
  ],
  "refresh": "10s",
  "schemaVersion": 39,
  "tags": ["claude-code", "ai-scry", "telemetry"],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "AI Scry: Dispatch",
  "uid": "ai-scry-dispatch",
  "version": 1
}
