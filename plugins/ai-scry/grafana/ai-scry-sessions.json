{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "-- Grafana --" },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [
    { "title": "Dispatch", "url": "/d/ai-scry-dispatch/", "type": "link", "icon": "bolt" },
    { "title": "Sessions", "url": "/d/ai-scry-sessions/", "type": "link", "icon": "list-ul" },
    { "title": "Prompts", "url": "/d/ai-scry-prompts/", "type": "link", "icon": "comment-alt" }
  ],
  "liveNow": true,
  "panels": [
    {
      "title": "Session Stats",
      "type": "stat",
      "gridPos": { "h": 4, "w": 24, "x": 0, "y": 0 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "refId": "Agents",
          "expr": "sum(count_over_time({service_name=\"ai-scry\"} | json | session_id=~\"$session_id\" | event_type=\"agent_complete\" [$__range]))",
          "legendFormat": "Agents"
        },
        {
          "refId": "Tools",
          "expr": "sum(count_over_time({service_name=\"ai-scry\"} | json | session_id=~\"$session_id\" | event_type=\"tool_start\" [$__range]))",
          "legendFormat": "Tools"
        },
        {
          "refId": "Errors",
          "expr": "sum(count_over_time({service_name=\"ai-scry\"} | json | session_id=~\"$session_id\" | event_type=\"tool_error\" [$__range]))",
          "legendFormat": "Errors"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 50 }
            ]
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Errors" },
            "properties": [
              { "id": "thresholds", "value": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "red", "value": 1 }] } }
            ]
          }
        ]
      },
      "options": {
        "reduceOptions": { "calcs": ["sum"], "fields": "", "values": false },
        "orientation": "horizontal",
        "textMode": "auto",
        "colorMode": "background"
      }
    },
    {
      "title": "Agent Dispatch Table",
      "type": "table",
      "gridPos": { "h": 12, "w": 24, "x": 0, "y": 4 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "refId": "A",
          "expr": "{service_name=\"ai-scry\"} | json | session_id=~\"$session_id\" | event_type=\"agent_complete\"",
          "legendFormat": ""
        }
      ],
      "transformations": [
        {
          "id": "extractFields",
          "options": {
            "source": "Line",
            "format": "json"
          }
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Line": true, "id": true, "labels": true, "tsNs": true, "labelTypes": true, "session_id": true, "event_type": true, "timestamp": true, "contributes_to": true, "transcript_path": true, "agent_transcript_path": true, "task_tags": true },
            "renameByName": {
              "agent_id": "Agent ID",
              "parent_agent_id": "Parent",
              "agent_label": "Type",
              "task_description": "Task",
              "model": "Model",
              "duration_ms": "Duration",
              "tokens_in": "Tokens In",
              "tokens_out": "Tokens Out",
              "status": "Status"
            }
          }
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Task" },
            "properties": [
              { "id": "custom.width", "value": 300 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Duration" },
            "properties": [
              { "id": "unit", "value": "ms" }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": [{ "displayName": "Time", "desc": true }]
      }
    },
    {
      "title": "Agent Activity",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 16 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (agent_label) (count_over_time({service_name=\"ai-scry\"} | json | session_id=~\"$session_id\" | event_type=~\"tool_start|tool_end\" [$__interval]))",
          "legendFormat": "{{agent_label}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 80,
            "gradientMode": "none",
            "lineWidth": 0,
            "barAlignment": 0,
            "showPoints": "never",
            "stacking": { "mode": "normal" }
          },
          "unit": "short"
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "right" },
        "tooltip": { "mode": "multi", "sort": "desc" }
      }
    },
    {
      "title": "Tool Timeline",
      "type": "logs",
      "gridPos": { "h": 10, "w": 16, "x": 0, "y": 24 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "refId": "A",
          "expr": "{service_name=\"ai-scry\"} | json | session_id=~\"$session_id\" | event_type=~\"tool_start|tool_end\"",
          "legendFormat": ""
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": false,
        "showCommonLabels": false,
        "wrapLogMessage": true,
        "prettifyLogMessage": true,
        "enableLogDetails": true,
        "sortOrder": "Descending",
        "dedupStrategy": "none"
      }
    },
    {
      "title": "Spawn Rate",
      "type": "timeseries",
      "gridPos": { "h": 10, "w": 8, "x": 16, "y": 24 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(count_over_time({service_name=\"ai-scry\"} | json | session_id=~\"$session_id\" | event_type=\"tool_start\" | tool_name=\"Task\" [$__interval]))",
          "legendFormat": "Agent Spawns"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 60,
            "gradientMode": "none",
            "lineWidth": 1,
            "barAlignment": 0,
            "showPoints": "never",
            "stacking": { "mode": "none" }
          },
          "unit": "short"
        }
      },
      "options": {
        "legend": { "displayMode": "hidden" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "title": "Tool Distribution",
      "type": "piechart",
      "gridPos": { "h": 10, "w": 12, "x": 0, "y": 34 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (tool_name) (count_over_time({service_name=\"ai-scry\"} | json | session_id=~\"$session_id\" | event_type=\"tool_start\" [$__range]))",
          "legendFormat": "{{tool_name}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" }
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["sum"], "fields": "", "values": false },
        "pieType": "donut",
        "tooltip": { "mode": "single" },
        "legend": { "displayMode": "table", "placement": "right", "values": ["value", "percent"] }
      }
    },
    {
      "title": "Error Stream",
      "type": "logs",
      "gridPos": { "h": 10, "w": 12, "x": 12, "y": 34 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "refId": "A",
          "expr": "{service_name=\"ai-scry\"} | json | session_id=~\"$session_id\" | event_type=\"tool_error\"",
          "legendFormat": ""
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": false,
        "showCommonLabels": false,
        "wrapLogMessage": true,
        "prettifyLogMessage": true,
        "enableLogDetails": true,
        "sortOrder": "Descending",
        "dedupStrategy": "none"
      }
    }
  ],
  "refresh": "10s",
  "schemaVersion": 39,
  "tags": ["claude-code", "ai-scry", "sessions"],
  "templating": {
    "list": [
      {
        "name": "session_id",
        "type": "query",
        "datasource": { "type": "loki", "uid": "loki" },
        "query": "label_values({service_name=\"ai-scry\"}, session_id)",
        "refresh": 2,
        "includeAll": true,
        "allValue": ".*",
        "current": { "text": "All", "value": "$__all" },
        "sort": 2,
        "label": "Session"
      }
    ]
  },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "AI Scry: Session Explorer",
  "uid": "ai-scry-sessions",
  "version": 1
}
